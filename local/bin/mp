#!/usr/bin/env python

import sys
import random
import string

def genpasswd(length=10, chars=(string.letters + string.digits + '_' + '-')):
    return ''.join([random.choice(chars) for i in xrange(0, length)])


if __name__ == '__main__':
    if len(sys.argv) > 1:
        print genpasswd(length=int(sys.argv[1]))
    else:
        print genpasswd()
